---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Open Source eUC">
	<div>
		<h1>osuc.dev</h1>
	</div>
</Layout>

<style>
	h1 {
		@apply text-transparent bg-clip-text text-8xl font-extrabold;
		background-image: linear-gradient(45deg, rgb(77, 87, 224), #75b2e0);
		background-position: 0%;
		background-size: contain;
	}
	div {
		@apply flex justify-center items-center h-screen;
	}
</style>
